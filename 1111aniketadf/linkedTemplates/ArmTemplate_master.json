{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "1111aniketadf"
		},
		"LS_BLOB_SRC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BLOB_SRC'"
		},
		"MyEventBasedTrig_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/da1e8c0b-b440-4931-b12c-7c26373e718f/resourceGroups/aniketrg/providers/Microsoft.Storage/storageAccounts/aniketsc"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "1111aniketadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BLOB_SRC_connectionString": {
						"value": "[parameters('LS_BLOB_SRC_connectionString')]"
					},
					"MyEventBasedTrig_properties_typeProperties_scope": {
						"value": "[parameters('MyEventBasedTrig_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}